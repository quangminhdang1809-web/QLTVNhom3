  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>;WITH AllMonths AS (
    -- Bước 1: Tạo một bảng tạm ảo chứa 12 số (từ 1 đến 12) đại diện cho 12 tháng.
    SELECT 1 AS Thang
    UNION ALL SELECT 2
    UNION ALL SELECT 3
    UNION ALL SELECT 4
    UNION ALL SELECT 5
    UNION ALL SELECT 6
    UNION ALL SELECT 7
    UNION ALL SELECT 8
    UNION ALL SELECT 9
    UNION ALL SELECT 10
    UNION ALL SELECT 11
    UNION ALL SELECT 12
),
MonthlyFines AS (
    -- Bước 2: Lấy dữ liệu phạt thực tế từ bảng PHIEUTHUTIENPHAT
    -- Nhóm theo tháng và tính tổng tiền của năm @p_Year
    SELECT
        MONTH(NgayThu) AS Thang,
        SUM(TongTienThu) AS TongTien
    FROM
        dbo.PHIEUTHUTIENPHAT
    WHERE
        YEAR(NgayThu) = @p_Year -- Đây là tham số SQL
    GROUP BY
        MONTH(NgayThu)
)
-- Bước 3: Kết hợp 2 bảng trên
-- Lấy tất cả 12 tháng từ AllMonths (bên trái)
-- và ghép (LEFT JOIN) với dữ liệu phạt từ MonthlyFines (bên phải).
SELECT
    AllMonths.Thang AS STT,  -- Cột 1: STT (từ 1 đến 12)
    N'Tháng ' + CAST(AllMonths.Thang AS VARCHAR(2)) AS TenThang, -- Cột 2: Tên tháng
    ISNULL(MonthlyFines.TongTien, 0) AS TongTienPhat -- Cột 3: Tổng tiền, ISNULL để chuyển NULL thành 0
FROM
    AllMonths
LEFT JOIN
    MonthlyFines ON AllMonths.Thang = MonthlyFines.Thang
ORDER BY
    AllMonths.Thang;</value>
  </data>
